{
  "name": "mosha-vue-toastify",
  "version": "0.0.0",
  "files": ["dist"],
  "main": "./dist/mosha-vue-toastify.umd.js",
  "module": "./dist/mosha-vue-toastify.es.js",
  "exports": {
    ".": {
      "import": "./dist/mosha-vue-toastify.es.js",
      "require": "./dist/mosha-vue-toastify.umd.js"
    }
  },
  "types": "dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -P tsconfig.json",
    "serve": "vite preview",
    "example:update": "yarn build && cd examples/vite-sample && yarn link \"mosha-vue-toastify\"",
    "example:dev": "cd examples/vite-sample && yarn dev"
  },
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.32",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "path": "^0.12.7",
    "typescript": "^4.1.3",
    "vite": "^2.0.5"
  }
}
